    	try {
            var removeTabindex = function() {
                console.log("Attempting to remove tabindex");
                if(document.getElementsByClassName('cCenterPanel').length !== 0) {
                    console.log("Inside if statement");
                    var centerPanel = document.getElementsByClassName('cCenterPanel')[0];
                    console.log("Ccenterpanel", centerPanel)
            		centerPanel.removeAttribute('tabindex');
                    console.log("Removed tabindex");
                } else {
                    window.setTimeout(function() {
                        removeTabindex();
                    }, 100);
                }

            }
            
